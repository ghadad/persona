{
  "name": "server2",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "test:ava": "npm run build:ts && ava  \"./dist/src/**/__test__/*.test.js/\"",
    "test2": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"src/**/__test__/*.test.ts\"",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\" && ava",
    "start": "npm run build:ts && fastify start -l info dist/src/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "rimraf ./dist && npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
